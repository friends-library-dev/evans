{
  "name": "@friends-library/evans",
  "description": "Friends Library website (English and Spanish)",
  "author": "Jared Henderson <jared@netrivet.com>",
  "repository": "github:friends-library-dev/evans",
  "private": true,
  "scripts": {
    "develop:en:site": "GATSBY_LANG=en GATSBY_PORT=9222 GATSBY_ALT_PORT=9333 gatsby develop --port 9222",
    "develop:es:site": "GATSBY_LANG=es GATSBY_PORT=9333 GATSBY_ALT_PORT=9222 gatsby develop --port 9333",
    "develop:en": "run-p develop:en:site develop:functions",
    "develop:es": "run-p develop:es:site develop:functions",
    "develop:functions": "cd ../functions && npm start",
    "build:functions": "cd ../functions && npm run build",
    "symlink:functions": "ln -s ../functions/dist functions",
    "postinstall": "npm run cover-css",
    "prebuild": "npm run cover-css",
    "predevelop:en:site": "npm run cover-css",
    "predevelop:es:site": "npm run cover-css",
    "build": "rm -rf ./functions && gatsby build && npm run build:functions && npm run symlink:functions && cp ./_redirects ./public/",
    "build:en": "GATSBY_LANG=en npm run build",
    "build:es": "GATSBY_LANG=es npm run build",
    "dpc-cache:clean": "rm -f src/build/.dpc-cache.json",
    "cover-css": "fldev tsnode ./src/build/cover-static-css.ts",
    "analyze": "rm -rf ./functions && ANALYZE_BUNDLE_SIZE=true npm run build:en",
    "start": "gatsby develop --port 9222",
    "test": "fldev test",
    "lint": "fldev lint",
    "lint:fix": "npm run lint -- --fix",
    "ts:check": "fldev ts:check",
    "format": "fldev format"
  },
  "dependencies": {
    "@friends-library/cover-component": "^2.0.0",
    "@friends-library/locale": "^1.0.0",
    "@friends-library/lulu": "^1.1.1",
    "@friends-library/types": "^1.1.2",
    "@reach/accordion": "^0.11.2",
    "@reach/dialog": "^0.11.2",
    "algoliasearch": "^4.5.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.24.66",
    "gatsby-background-image-preact": "^1.1.2-alpha.1",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-manifest": "^2.4.32",
    "gatsby-plugin-mdx": "^1.2.44",
    "gatsby-plugin-preact": "^4.0.15",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-sharp": "^2.6.37",
    "gatsby-plugin-typescript": "^2.4.21",
    "gatsby-source-filesystem": "^2.3.31",
    "gatsby-transformer-sharp": "^2.5.16",
    "js-cookie": "^2.2.1",
    "lodash.throttle": "^4.1.1",
    "preact": "^10.5.4",
    "preact-render-to-string": "^5.1.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-instantsearch-dom": "^6.7.0",
    "react-stripe-elements": "^6.1.2",
    "react-swipeable": "^5.5.1",
    "react-switch": "^5.0.1",
    "really-small-events": "^1.1.0",
    "smoothscroll-polyfill": "^0.4.4",
    "use-interval": "^1.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@friends-library/adoc-convert": "^1.1.2",
    "@friends-library/dev": "^4.1.9",
    "@friends-library/document-meta": "^1.0.0",
    "@friends-library/dpc-fs": "^1.0.0",
    "@friends-library/env": "^1.2.0",
    "@friends-library/friend-repos": "^1.1.0",
    "@friends-library/friends": "^1.3.0",
    "@friends-library/theme": "^1.1.0",
    "@types/classnames": "^2.2.10",
    "@types/fs-extra": "^9.0.1",
    "@types/he": "^1.1.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-instantsearch-dom": "^6.3.0",
    "@types/react-stripe-elements": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "babel-plugin-typescript-strip-namespaces": "^1.1.1",
    "babel-preset-gatsby": "^0.5.13",
    "eslint": "^7.9.0",
    "fs-extra": "^9.0.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-postcss": "^3.0.1",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.13",
    "gatsby-plugin-sitemap": "^2.4.15",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "he": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "tailwindcss": "^1.8.10",
    "x-chalk": "^1.0.1"
  }
}
